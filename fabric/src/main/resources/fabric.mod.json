{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "icon": "assets/releaserewards/${mod_id}_icon.png",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "sources": "https://github.com/anabsolutesloth/cobblemonreleaserewards",
    "issues": "https://github.com/anabsolutesloth/cobblemonreleaserewards/issues"
  },
  "license": "${mod_license}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.emperdog.releaserewards.fabric.ReleaseRewardsFabricMod"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=21",
    "architectury": "${architectury_version}.x",
    "cobblemon": ">=${cobblemon_version}"
  }
}
